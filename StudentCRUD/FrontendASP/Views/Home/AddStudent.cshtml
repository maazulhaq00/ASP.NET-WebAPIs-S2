@{
}

<form method="post">
    <div class="mb-3">
        <input asp-for="studentName" class="form-control" placeholder="Student Name">
    </div>
  <div class="mb-3">
        <input asp-for="studentAge" class="form-control" placeholder="Age">
    </div>
    <div class="mb-3">
        <input asp-for="studentBatchCode" class="form-control" placeholder="Batch Code">
    </div>
    <div class="mb-3">
        <input asp-for="studentPhone" class="form-control" placeholder="Student Phone">
    </div>
    <div class="mb-3">
        <input asp-for="studentGender" class="form-control" placeholder="Gender">
    </div>
    <button type="submit">Add</button>
  </form>
